.gg.bookmark, .gg.bookmark::after
  display block
  box-sizing border-box
.gg.bookmark
  border 2px solid
  border-bottom 0
  overflow hidden
  position relative
  width 14px
  height 16px
.gg.bookmark::after
  content ''
  position absolute
  width 12px
  height 12px
  border-top 2px solid
  border-right 2px solid
  transform rotate(-45deg)
  top 9px
  left -1px
