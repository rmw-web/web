.gg.im
  transform scale(0.6)
  box-sizing border-box
  position relative
  display block
  width 20px
  height 17px
  border 2px solid
  border-bottom 0
  box-shadow -6px 8px 0 -6px, 6px 8px 0 -6px
  transform-origin 50% 43%

.gg.im::after, .gg.im::before
  content ''
  display block
  box-sizing border-box
  position absolute
  width 8px

.gg.im::before
  border 2px solid
  border-top-color transparent
  border-bottom-left-radius 20px
  right 4px
  bottom -6px
  height 6px

.gg.im::after
  height 2px
  background currentColor
  box-shadow 0 4px 0 0
  left 4px
  top 4px
