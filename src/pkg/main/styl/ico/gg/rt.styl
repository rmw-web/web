.gg.rt
  box-sizing border-box
  position relative
  display block
  box-shadow -2px -2px 0 0, 2px 2px 0 0
  width 14px
  height 6px

.gg.rt::after, .gg.rt::before
  content ''
  display block
  box-sizing border-box
  position absolute
  width 0
  height 0
  border-top 3px solid transparent
  border-bottom 3px solid transparent

.gg.rt::before
  border-left 5px solid
  top -4px
  right 0

.gg.rt::after
  border-right 5px solid
  bottom -4px
  left 0
